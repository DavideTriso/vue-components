@import "./../../../assets/stylus/helpers/spa-imports.styl"

.panel
  +has('title')
    font-size: 1rem
    font-weight: 400
    margin: 0
    padding: 0
    +variant('primary')
      color: $white
    +variant('success')
      color: $white
    +variant('danger')
      color: $white
    +variant('warning')
      color: $white
    +variant('info')
      color: $white
    +variant('dark')
      color: $white
